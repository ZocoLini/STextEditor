{
  "rules": [
    {
      "name": "method",
      "styleClass": "method",
      "regex": "[a-zA-Z_$][a-zA-Z0-9_$]*\\(",
      "highlightRegex": "[a-zA-Z_$][a-zA-Z0-9_$]*"
    },
    {
      "name": "tag",
      "styleClass": "tag",
      "regex": "@[a-zA-Z0-9_$]+",
      "highlightRegex": ""
    },
    {
      "name": "string",
      "styleClass": "string",
      "regex": "\"[^\"]*\"",
      "highlightRegex": ""
    },
    {
      "name": "number",
      "styleClass": "number",
      "regex": "\\b[0-9]+\\b",
      "highlightRegex": ""
    },
    {
      "name": "comment",
      "styleClass": "comment",
      "regex": "//[^\\n]*|/\\*(.|\\n)*\\*/",
      "highlightRegex": ""
    },
    {
      "name": "keywordsList",
      "styleClass": "keyword",
      "regex": "\\b(?:public|private|abstract|protected|static|final|void|int|boolean|return|true|false|null|this|new|class|extends|implements|interface|package|import|super|try|catch|finally|throw|throws|assert|break|continue|case|default|do|enum|goto|instanceof|native|const)\\b",
      "highlightRegex": ""
    }
  ]
}